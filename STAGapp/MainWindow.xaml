<Window x:Class="STAGapp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:STAGapp"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1480"
        Loaded="Window_Loaded">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
        </StackPanel>
        <Frame x:Name="Main" Margin="0, 35, 0, 0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
